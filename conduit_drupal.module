/**
 * Implements hook_ctools_plugin_directory().
 */
function conduit_drupal_ctools_plugin_directory($module, $plugin) {
  if (($module == 'worker' && $plugin == 'worker') || ($module == 'conduit' && $plugin == 'server')) {
    return 'plugins/' . $plugin;
  }
}
